<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.7.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/1.7.0/distro-source/core/src/gwt-module.dtd">
<module rename-to='home'>
	<!-- Inherit the core Web Toolkit stuff.                        -->
	<inherits name="com.bkitmobile.poma.captcha.captcha" />
	<inherits name="com.bkitmobile.poma.database.database" />
	<inherits name="com.bkitmobile.poma.flexmap.flexmap" />
	<inherits name="com.bkitmobile.poma.localization.localization" />
	<inherits name="com.bkitmobile.poma.mail.mail" />
	<inherits name="com.bkitmobile.poma.mobile.mobile" />
	<inherits name="com.bkitmobile.poma.openid.openid" />
	<inherits name="com.bkitmobile.poma.ui.ui" />
	<inherits name="com.bkitmobile.poma.util.util" />

	<!-- English language, independent of country -->
	<extend-property name="locale" values="en" />

	<!-- Vietnamese language -->
	<extend-property name="locale" values="vi" />

	<!-- Specify the app entry point class. -->
	<entry-point class="com.bkitmobile.poma.home.client.BkitPoma" />
	
<!--	 User Agent - specified browser for fast compile speed-->
<!--    <set-property name="user.agent" value="gecko" />-->
</module>